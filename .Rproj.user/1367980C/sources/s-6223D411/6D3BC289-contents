#Plot deamination
deam_freqs <- read.table("PMD_temp.txt", header = T)

par(mfrow=c(1,2))
plot(deam_freqs$z, deam_freqs$CT, type = 'l', col = 'red', lwd = 2)
lines(deam_freqs$z, deam_freqs$CA)
lines(deam_freqs$z, deam_freqs$CG, col = 'blue')

plot(-deam_freqs$z, deam_freqs$GA, type = 'l', col = 'blue', lwd =2)
lines(-deam_freqs$z, deam_freqs$GT)
lines(-deam_freqs$z, deam_freqs$GC)

